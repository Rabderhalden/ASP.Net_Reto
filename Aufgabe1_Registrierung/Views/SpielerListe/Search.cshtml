@model IEnumerable<Aufgabe1_Registrierung.Models.Spieler>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Suche";
}

<!DOCTYPE html>

<H1>Gefundene Spieler:</H1>

<table>
    <tr>
        <th>Vorname:</th>
        <th>Nachname:</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.vorname </td>
            <td>@item.nachname</td>
        </tr>
    }
</table>
<br />
<br />
@Html.DropDownList("Spielerliste","Wähle Spieler")

